			  		return false;
			  	}
			  }
			  if (mm==2)
			  {
			  	var lyear = false;
			  	if ( (!(yy % 4) && yy % 100) || !(yy % 400)) 
			  	{
			  		lyear = true;
			  	}
			  	if ((lyear==false) && (dd>=29))
			  	{
			  	alert('Invalid date format!');
			  	return false;
			  	}
			  	if ((lyear==true) && (dd>29))
